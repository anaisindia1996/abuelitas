        <%= simple_form_for @experience do |f| %>
        <%= f.input :grandma_name %>
        <%= f.input :activity_name %>
        <%= f.input :availability %>
        <%= f.input :description %>
        <%= f.input :price %>
        <%= f.input :location %>
        <%= f.input :latitude %>
        <%= f.input :longitude %>
        <%= f.input :photo, as: :file %>
        <%= f.submit class: 'btn btn-primary' %>
      <% end %>
